              a.button.color.big.round(href='NewClient') Add New Client
              a.button.color.big.round(href='#') Delete Client
            .blog-post.layout-2
              div(style='float: left;width:20px')
                input(type='checkbox')
              .permalink
                a(href='ClientDetail') Client1
            .blog-post.layout-2
              div(style='float: left;width:20px')
                input(type='checkbox')
              .permalink
                a(href='ClientDetail') Client2
